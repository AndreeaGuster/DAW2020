@{
    ViewBag.Title = "Users"; }
<h2>@ViewBag.Title</h2>
<table class="table">
    <thead>
    <th>Username</th>
    <th>Email</th>
    <th>Update</th>
    <th>Details</th>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.UsersList)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@Html.ActionLink("Update", "Edit", new { id = user.Id })</td>
                <td>@Html.ActionLink("View details", "Details", new { id = user.Id })</td>
            </tr>
        }
    </tbody>
</table>